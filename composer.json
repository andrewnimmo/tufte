{
  "require-dev": {
    "squizlabs/php_codesniffer": "2.8.1",
    "wp-coding-standards/wpcs": "0.11.0"
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
      "./vendor/bin/phpcs --standard=WordPress *.php */*.php --ignore=vendor"
    ]
  }
}
